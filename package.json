    "googleapis": "^95.0.0",
    "helmet": "^4.6.0",
    "ip2location-nodejs": "^9.4.2",
    "ipaddr.js": "^2.1.0",
    "js-yaml": "^4.1.0",
    "json2csv": "^5.0.7",
    "moment": "^2.29.1",
    "mongodb": "^4.2.0",
    "nats": "^2.7.1",
    "nestjs-form-data": "^1.8.3",
    "nestjs-real-ip": "^2.2.0",
    "nestjs-redoc": "^2.2.2",
    "node-device-detector": "^2.0.12",
    "octokit": "^1.7.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "saml2js": "^0.1.2",
    "sha256-file": "^1.0.0",
    "slugify": "^1.6.5",
