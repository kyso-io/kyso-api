    "@nestjs/swagger": "^7.0.12",
    "@octokit/rest": "^19.0.13",
    "@opentelemetry/instrumentation-nestjs-core": "^0.32.5",
    "@opentelemetry/sdk-trace-node": "^1.14.0",
    "@willsoto/nestjs-prometheus": "^5.2.0",
    "adm-zip": "^0.5.10",
    "api-query-params": "^5.4.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "fast-glob": "^3.2.12",
    "fast-xml-parser": "^4.2.5",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "geoip-country": "^4.1.65",
    "glob": "^10.3.1",
    "googleapis": "^120.0.0",
    "helmet": "^7.0.0",
    "ip2location-nodejs": "^9.5.0",
    "ipaddr.js": "^2.1.0",
    "js-yaml": "^4.1.0",
    "json2csv": "^5.0.7",
    "moment": "^2.29.4",
    "mongodb": "^5.6.0",
    "nats": "^2.15.1",
    "nestjs-form-data": "^1.9.0",
    "nestjs-real-ip": "^3.0.1",
    "node-device-detector": "^2.0.14",
    "octokit": "^2.1.0",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "saml2js": "^0.1.2",
    "sha256-file": "^1.0.0",
    "slugify": "^1.6.6",
    "ssh2-sftp-client": "^9.1.0",
    "swagger-ui-express": "^4.6.3",
