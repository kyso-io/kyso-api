    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@willsoto/nestjs-prometheus": "^4.4.0",
    "adm-zip": "^0.5.9",
    "api-query-params": "^5.3.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "fast-glob": "^3.2.7",
    "fast-xml-parser": "^4.1.2",
