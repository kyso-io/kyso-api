  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@nestjs/cli": "^10.1.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.4",
    "@types/adm-zip": "^0.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/geoip-country": "^4.0.0",
    "@types/get-folder-size": "^3.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/json2csv": "^5.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.2",
    "@types/sha256-file": "^1.0.1",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "json-schema-faker": "^0.5.3",
    "normalizr": "^3.6.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "widdershins": "^4.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../.jest/setEnvVars.js"
    ],
    "verbose": true,
    "testTimeout": 60000
  }
}
